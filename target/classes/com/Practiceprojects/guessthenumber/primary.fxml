<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vbox" alignment="CENTER" fillWidth="false" prefHeight="280.0" prefWidth="412.0" spacing="20.0" style="-fx-background-color: #40e0d0;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Practiceprojects.guessthenumber.Controller">
   <children>
      <Label text="I am thinking of a number between 1-100. ">
         <font>
            <Font size="19.0" />
         </font></Label>
      <Label text="Can you guess it?">
         <font>
            <Font size="19.0" />
         </font></Label>
      <TextField fx:id="textField" prefHeight="47.0" prefWidth="58.0">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button fx:id="button" mnemonicParsing="false" onAction="#buttonPressed" text="Guess" />
      <Label fx:id="msg1" text="">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label fx:id="msg2">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label fx:id="msg3">
         <font>
            <Font size="14.0" />
         </font></Label>
   
   </children>
   
</VBox>
